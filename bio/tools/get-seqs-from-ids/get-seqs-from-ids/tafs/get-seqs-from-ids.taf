+TOOL:get-seqs-from-ids
ARGS
### Optional ##############################################################################
    <else>
        cat ::*LOAD-DIR*::../get-seqs-from-ids.info.txt
    <type>
        # load & read, load can load files in memory then work, read can work with read line by line
        # load may need more memory, read don't
        read
    <cl-file>
        ::type::-lines.lisp
    <out>
        false
    <cmd>
        hanky-lisp
    <main>
        if [ "::out::" = "false" ]; then ::cmd:: --script ::*LOAD-DIR*::code/::cl-file:: ::seqs:: ::ids:: ; else ::cmd:: --script ::*LOAD-DIR*::code/::cl-file:: ::seqs:: ::ids:: > ::out:: ; fi
### NEED ##############################################################################
    <seqs>
        # seqs' fasta file
    <ids>
        # gene ID's file
RUN
    <container:taf-hanky-lisp:docker.io/taffish/hanky-lisp:latest>
        ::*MAIN*::
